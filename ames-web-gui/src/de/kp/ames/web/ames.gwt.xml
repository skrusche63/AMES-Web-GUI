<?xml version="1.0" encoding="UTF-8"?>

<module rename-to='adf_client'>
  
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.http.HTTP"/>
  <inherits name="com.google.gwt.json.JSON"/>
  
  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.clean.Clean'/>

  <!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Other module inherits                                      -->
  <inherits name="com.smartgwt.SmartGwt"/>
  <!-- <inherits name="com.smartclient.theme.fleet.Fleet"/> -->
  <!-- Access to Developer Console                                -->
  <inherits name="com.smartgwt.tools.SmartGwtTools"/>
  
  <inherits name='de.kp.ames.web.shared.constants'/> 
  <inherits name='de.kp.ames.map.openlayers'/> 
  
  <!-- Drawing 													  -->
  <inherits name="com.smartgwt.Drawing" />
  
  <!-- thejit                                                     -->
  <inherits name="de.kp.ames.thejit.thejit"/>
  
  
  <!-- Specify the app entry point class.                         -->
  <entry-point class='de.kp.ames.web.client.main'/>
 
  <!-- Optimize compile permutation during development.            -->
  <!-- <set-property name="user.agent" value="safari,gecko1_8" /> -->
  <set-property name="user.agent" value="gecko1_8" />

</module>
